{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "tallcms/cms",
    "type": "library",
    "version": "2.4.2",
    "description": "TallCMS - A modern CMS for Filament admin panels. Works standalone or as a plugin in existing Filament apps.",
    "keywords": [
        "cms",
        "filament",
        "filament-plugin",
        "laravel",
        "livewire",
        "tallstack",
        "content-management",
        "blog",
        "pages"
    ],
    "license": "MIT",
    "homepage": "https://tallcms.com",
    "support": {
        "issues": "https://github.com/tallcms/cms/issues",
        "source": "https://github.com/tallcms/cms",
        "docs": "https://tallcms.com/docs"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/tallcms"
        }
    ],
    "authors": [
        {
            "name": "TallCMS",
            "email": "hello@tallcms.com",
            "homepage": "https://tallcms.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "lara-zeus/spatie-translatable": "^1.0",
        "spatie/laravel-package-tools": "^1.16",
        "bezhansalleh/filament-shield": "^4.0",
        "spatie/laravel-permission": "^6.0",
        "ezyang/htmlpurifier": "^4.19",
        "paragonie/sodium_compat": "^2.0",
        "wsmallnews/filament-nestedset": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0 || ^10.0",
        "phpunit/phpunit": "^10.5 || ^11.0",
        "spatie/laravel-translatable": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TallCms\\Cms\\": "src/",
            "TallCms\\Cms\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TallCms\\Cms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TallCms\\Cms\\TallCmsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
